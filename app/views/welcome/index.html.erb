  <div class="container w-full px-6 py-4 pb-5 bg-green-800 flex items-center text-white rounded-lg justify-between">
    <% if @categories.count > 0 %>
      <h2 class="text-2xl font-semibold">Available Categories:</h2>
    <% else %>
      <h2 class="text-2xl font-semibold">There are no available Categories yet, create one now.</h2>
    <% end %>
    <div class="flex-row flex">
      <% if @categories.count > 0 %>
        <button class="flex items-center px-6 py-2 mt-auto font-semibold text-black transition duration-500 ease-in-out transform bg-yellow-200 rounded-lg hover:bg-yellow-300 focus:shadow-outline focus:outline-none focus:ring-2 ring-offset-current ring-offset-2 text-lg mr-3">
          <%= link_to "Show all Categories" , categories_path, title:"Show All Categories" %>
        </button>
      <% end %>
      <button class="flex items-center px-6 py-2 mt-auto font-semibold text-black transition duration-500 ease-in-out transform bg-yellow-200 rounded-lg hover:bg-yellow-300 focus:shadow-outline focus:outline-none focus:ring-2 ring-offset-current ring-offset-2 text-lg">
        <%= link_to "Add a Category" , new_category_path, title:"Add a New Task" %>
      </button>
    </div>
  </div>

  <% if @categories.count > 0 %>
  <div class="container w-full grid grid-cols-4 gap-x-4 gap-y-6 my-6">
    <% @categories.each do |category| %>
      <div class="w-full bg-white border rounded-lg shadow-xl relative">
        <div class="py-2 px-4">
          <h2 class="pt-4 mb-4 text-xs font-semibold tracking-widest text-black uppercase title-font">Pending Tasks: <%= category.tasks.count %></h2>
          <h4 class="mb-4 text-2xl font-semibold leading-none tracking-tighter text-black title-font hover:text-yellow-400 "><%= link_to category.name, category_tasks_path(category), title: "Show Category" %></h4>
          <p class="mb-10 text-base leading-relaxed text-gray-500"><%= category.description %></p>
          <div class="flex flex-wrap justify-end items-center absolute bottom-4 right-4">
            <%= link_to category_tasks_path(category), title: "Show Category", class: "ml-2 text-green-800 hover:text-green-800" do %>
              <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                <path d="M9 4.804A7.968 7.968 0 005.5 4c-1.255 0-2.443.29-3.5.804v10A7.969 7.969 0 015.5 14c1.669 0 3.218.51 4.5 1.385A7.962 7.962 0 0114.5 14c1.255 0 2.443.29 3.5.804v-10A7.968 7.968 0 0014.5 4c-1.255 0-2.443.29-3.5.804V12a1 1 0 11-2 0V4.804z" />
              </svg>
            <% end %>
            <%= link_to edit_category_path(category), title: "Edit Category", class: "ml-2 text-green-800 hover:text-green-800" do %>
              <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                <path d="M17.414 2.586a2 2 0 00-2.828 0L7 10.172V13h2.828l7.586-7.586a2 2 0 000-2.828z" />
                <path fill-rule="evenodd" d="M2 6a2 2 0 012-2h4a1 1 0 010 2H4v10h10v-4a1 1 0 112 0v4a2 2 0 01-2 2H4a2 2 0 01-2-2V6z" clip-rule="evenodd" />
              </svg>
            <% end %>
            <%= link_to category_path(category), title: "Delete Category", class: "ml-2 text-red-600" , method: :delete, data: {
              confirm: "Are you sure?" } do %>
              <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                <path fill-rule="evenodd" d="M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z" clip-rule="evenodd" />
              </svg>
            <% end %>
          </div>
        </div>
      </div>
    <% end %>
    <% if @categories2.count > 11 %>
      <div class="w-full bg-green-400 border rounded-lg shadow-xl relative grid items-center justify-center">
        <div class="py-2 px-4">
          <%= link_to categories_path, title: "Show Categories" do %>
            <h4 class="mb-4 text-2xl font-semibold leading-none tracking-tighter text-white title-font hover:text-yellow-200 ">See all <%= @categories2.count %> Categories</h4>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
